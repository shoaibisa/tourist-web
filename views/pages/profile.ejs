<%- include("../conn/header.ejs") %> <%- include("../conn/Top.ejs") %>
<!-- Main Sidebar Container -->
<%- include("../conn/navebar.ejs") %>
<div class="content-wrapper">
  <section class="content-header">
    <div class="container-fluid">
      <h1>Profile</h1>
    </div>
  </section>
  <!-- Main content -->
  <section class="content">
    <div class="container-fluid">
      <div class="row">
        <div class="col-md-3">
          <div class="card card-primary card-outline">
            <div class="card-body box-profile">
              <div class="text-center">
                <img
                  class="profile-user-img img-fluid img-circle"
                  src="../../dist/img/user4-128x128.jpg"
                  alt="User profile picture"
                />
              </div>
              <h3 class="profile-username text-center"><%=guide.guideName%></h3>
              <p class="text-muted text-center">Guide</p>
            </div>
          </div>
        </div>
        <div class="col-md-9">
          <div class="card">
            <div class="row card-body">
              <div class="col">
                <strong><i class="fas fa-user mr-1"></i> Name</strong>
                <p>Anubhav Rajput</p>
              </div>
              <div class="col">
                <strong><i class="fas fa-mail-bulk mr-1"></i>Email</strong>
                <p><%=guide.guideEmail%></p>
              </div>
              <div class="col">
                <strong><i class="fas fa-phone mr-1"></i> Phone</strong>
                <p>8877949888</p>
              </div>
              <div class="col">
                <strong><i class="fas fa-city mr-1"></i>City</strong>
                <p>Delhi</p>
              </div>
            </div>
            <div class="row card-body">
              <div class="col">
                <strong
                  ><i class="fas fa-birthday-cake mr-1"></i>Date of
                  Birth</strong
                >
                <p>13/10/2000</p>
              </div>
              <div class="col">
                <strong><i class="fas fa-book mr-1"></i>Education</strong>
                <p>Btech</p>
              </div>
              <div class="col">
                <strong><i class="fas fa-country mr-1"></i>Country</strong>
                <p>India</p>
              </div>
              <div class="col">
                <strong
                  ><i class="fas fa-organization mr-1"></i>Organization</strong
                >
                <p>SLIET</p>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
</div>
<!-- /Footer -->
